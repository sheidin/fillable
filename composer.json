{
    "name": "sheidin/fillable",
    "description": "Generate fillable attribute for eloquent models",
    "keywords": [
        "sheidin",
        "laravel",
        "fillable"
    ],
    "homepage": "https://github.com/sheidin/fillable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Sheidin",
            "email": "teivat.mail@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sheidin\\Fillable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sheidin\\Fillable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sheidin\\Fillable\\FillableServiceProvider"
            ],
            "aliases": {
                "Fillable": "Sheidin\\Fillable\\Facades\\Fillable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
